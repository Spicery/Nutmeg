{
  "literals": {
    "NUM": "(?P<NUM>\\d+)",
    "STRING": "(?P<STRING>\".*\"|'.*')",
    "WHITESPACE": "(?P<WS>\\s+)"
  },
  "operators": {
    "BIND": "(?P<BIND>:=)",
    "ASSIGNMENT": "(?P<ASSIGN><-)",
    "UPDATE_ELEMENT": "(?P<UPDATE_ELEMENT><--)",
    "COPY_AND_SET": "(?P<COPY_AND_SET><==)",
    "PLUS": "(?P<PLUS>/+)",
    "MINUS": "(?P<MINUS>-)",
    "TIMES": "(?P<TIMES>\\*)",
    "DIVIDE": "(?P<DIVIDE>//)"
  },
  "separators": {
    "TERMINATE_STATEMENT": "(?P<TERMINATE_STATEMENT>;)",
    "LPAREN": "(?P<LPAREN>\\()",
    "RPAREN": "(?P<RPAREN>\\))"
  },
  "keywords": {
    "DEC_VARIABLE": "(?P<DEC_VARIABLE>var)",
    "DEC_IMMUTABLE": "(?P<DEC_IMMUTABLE>val)",
    "DEC_CONSTANT": "(?P<DEC_CONSTANT>const)",
    "DEC_FUNCTION_1": "(?P<DEC_FUNCTION_1>def)",
    "DEC_FUNCTION_2": "(?P<DEC_FUNCTION_2>function)"
  },
  "comments": {
    "COMMENT_LINE": "(?P<COMMENT_LINE>###)",
    "COMMENT_BLOCK_START": "(?P<COMMENT_BLOCK_START>##/()",
    "COMMENT_BLOCK_FINISH": "(?P<COMMENT_BLOCK_FINISH>##))"
  }
}
